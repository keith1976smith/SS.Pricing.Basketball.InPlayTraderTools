  "eslintConfig": {

	    "parser": "babel-eslint",

	    "env": {
	      "browser": true,
	      "node": true,
	      "es6": true
	    },

	    "ecmaFeatures": {
	      "jsx": true
	    },

	    "rules": {

	      "no-multi-spaces": 0,
	      "quotes": 0,
